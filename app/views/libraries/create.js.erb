$("#libraries_<%= @library.public ? 'public' : 'private' %>").append("<li><a href=\"<%= escape_javascript(user_library_elements_path(user_id: current_user.id, library_element: {article_id: resource.id, library_id: @library.id})) %>\" rel=\"nofollow\" data-method=\"post\"><i class=\"fa fa-eye<%= '-slash ' unless @library.public %> fa-lg\"></i>\n<%= @library.name %>\n</a></li>");
